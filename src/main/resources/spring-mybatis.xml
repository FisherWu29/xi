<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <environments default="">
        <environment id="">
            <transactionManager type=""></transactionManager>
            <dataSource type=""></dataSource>
        </environment>
    </environments>
    <!--<settings>-->
        <!--&lt;!&ndash;&lt;!&ndash; 全局映射器启用缓存 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="cacheEnabled" value="true"/>&ndash;&gt;-->

        <!--&lt;!&ndash;&lt;!&ndash; 查询时，关闭关联对象即时加载以提高性能 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; <setting name="lazyLoadingEnabled" value="true" /> &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash;&ndash;&gt;-->
            <!--&lt;!&ndash;设置关联对象加载的形态，此处为按需加载字段(加载字段由SQL指 定)，不会加载关联表的所有字段，以提高性能&ndash;&gt;-->
        <!--&lt;!&ndash;&ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 对于未知的SQL查询，允许返回不同的结果集以达到通用的效果 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="multipleResultSetsEnabled" value="true"/>&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 允许使用列标签代替列名 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="useColumnLabel" value="true"/>&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 允许使用自定义的主键值(比如由程序生成的UUID 32位编码作为键值)，数据表的PK生成策略将被覆盖 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="useGeneratedKeys" value="true"/>&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 给予被嵌套的resultMap以字段-属性的映射支持 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="autoMappingBehavior" value="FULL"/>&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 对于批量更新操作缓存SQL以提高性能   会影响返回行数&ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="defaultExecutorType" value="SIMPLE"/>&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 数据库超过25000秒仍未响应则超时 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="defaultStatementTimeout" value="25000"/>&ndash;&gt;-->

        <!--&lt;!&ndash;<setting name="logImpl" value="LOG4J"/>&ndash;&gt;-->
    <!--</settings>-->
    <mappers>
        <mapper resource="classpath:mybatis/PersonMap.xml"/>
    </mappers>
</configuration>